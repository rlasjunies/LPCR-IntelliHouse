<?xml version="1.0" encoding="<#encode#>"?>
<rdf:RDF
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns="http://purl.org/rss/1.0/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:sy="http://purl.org/rss/1.0/modules/syndication/">
	<channel rdf:about="<#alarm_url#>">
		<title>
			Alarm Information
		</title>
		<link>
			<#alarm_url#>
		</link>
		<description>
			<#description#>
		</description>
		<dc:language>
			<#language#>
		</dc:language>
		<dc:date>
			<#date#>
		</dc:date>
		<sy:updatePeriod>
			hourly
		</sy:updatePeriod>
		<sy:updateFrequency>
			60
		</sy:updateFrequency>
		<sy:updateBase>
			<#date#>
		</sy:updateBase>
		<items>
			<rdf:Seq>
				<##$rdf_seq_item_count$
				<rdf:li rdf:resource="<#rdf_item_resource#>"/>
				##>
			</rdf:Seq>
		</items>
	</channel>
	<##$rdf_seq_item_count$
	<item rdf:about="<#rdf_item_resource#>">
		<title>
			<#item_title#>
		</title>
		<link>
			<#item_link#>
		</link>
		<category>
			<#item_category#>
		</category>
		<description>
			<#item_description#>
		</description>
		<dc:subject>
			<#item_dc_subject#>
		</dc:subject>
		<dc:creator />
		<dc:date>
			<#item_dc_date#>
		</dc:date>
	</item>
	##>
</rdf:RDF>
